{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
	  {
		"name": "client",
		"image": "estudio05/client",
		"hostname": "client",
		"essential": false,
		"memory": 128,
		"portMappings": [
		{
			"hostPort": 8081,
			"containerPort": 8081
		}]
	  },
	  {
		"name": "admin",
		"image": "estudio05/admin",
		"hostname": "admin",
		"essential": false,
		"memory": 128,
		"portMappings": [
		{
			"hostPort": 8082,
			"containerPort": 8082
		}]
	  },
	  {
		"name": "db",
		"image": "estudio05/db",
		"hostname": "db",
		"essential": true,
		"memory": 128
	  },
	  {
		"name": "proxy",
		"image": "estudio05/proxy",
		"hostname": "proxy",
		"essential": false,
		"portMappings": [
		{
			"hostPort": 80,
			"containerPort": 80
		}],
		"links": ["client","admin"],
		"memory": 128
	  },
	  {
		"name": "adminer",
		"image": "adminer",
		"hostname": "adminer",
		"essential": false,
		"portMappings": [
		{
			"hostPort": 8083,
			"containerPort": 8083
		}],
		"links": ["db"],
		"memory": 128
	  }
	]
}

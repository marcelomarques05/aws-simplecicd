{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
	  {
		"name": "client",
		"image": "estudio05/client",
		"hostname": "client",
		"essential": false,
		"memory": 128
	  },
	  {
		"name": "admin",
		"image": "estudio05/admin",
		"hostname": "client",
		"essential": false,
		"memory": 128
	  },
	  {
		"name": "db",
		"image": "estudio05/db",
		"hostname": "client",
		"essential": true,
		"memory": 128
	  },
	  {
		"name": "proxy",
		"image": "estudio05/proxy",
		"hostname": "proxy",
		"essential": false,
		"portMappings": [
		{
			"hostPort": 80,
			"containerPort": 80
		}],
		"links": ["client","admin"],
		"memory": 128
	  }
	]
}
